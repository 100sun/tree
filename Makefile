c:
	gcc tree.c -Wall -o ctree
	./ctree

elixir:
	elixir tree.exs

go:
	go run tree.go

java:
	javac Tree.java
	java Tree

javascript:
	node tree.js

php:
	php tree.php

python:
	python tree.py

ruby:
	ruby tree.rb

rust:
	rustc tree.rs -o rusttree
	./rusttree

scala:
	scalac Tree.scala
	scala Tree

shell:
	sh tree.sh
